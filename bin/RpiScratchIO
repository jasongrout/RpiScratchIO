#!/usr/bin/env python
#
import sys,time
from ScratchIO import ScratchIO

if __name__ == "__main__":
  configFile = "RpiScratchIO.cfg"
  if len(sys.argv) > 1:
    configFile = sys.argv[1]

  r = ScratchIO(configFile)
  try:
    while 1:
      time.sleep(1000)

  except KeyboardInterrupt:
    r.cleanup()
